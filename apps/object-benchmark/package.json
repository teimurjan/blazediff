{
	"name": "@blazediff/object-benchmark",
	"version": "0.0.1",
	"description": "Object diffing benchmarks for BlazeDiff",
	"private": true,
	"files": [
		"dist"
	],
	"scripts": {
		"typecheck": "tsc --noEmit",
