{
	"name": "@blazediff/jest",
	"version": "1.0.0",
	"description": "Jest matcher for visual regression testing with blazediff",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"typecheck": "tsc --noEmit",
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rm -rf dist",
		"test": "jest"
	},
	"keywords": [
		"jest",
		"image",
		"comparison",
		"diff",
		"visual-testing",
		"matcher",
		"snapshot"
	],
	"author": "Teimur Gasanov <me@teimurjan.dev> (https://github.com/teimurjan)",
	"repository": "https://github.com/teimurjan/blazediff",
	"homepage": "https://blazediff.dev",
	"license": "MIT",
	"dependencies": {
		"@blazediff/matcher": "workspace:*"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^24.3.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.6",
		"tsup": "8.5.0",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"jest": ">=27.0.0"
	}
}
