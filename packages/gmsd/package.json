{
	"name": "@blazediff/gmsd",
	"version": "0.1.0",
	"description": "Fast single-threaded GMSD (Gradient Magnitude Similarity Deviation) metric for CI visual testing",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rm -rf dist",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [
		"image",
		"comparison",
		"gmsd",
		"gradient",
		"similarity",
		"visual-testing",
		"ci"
	],
	"author": "Teimur Gasanov <me@teimurjan.dev> (https://github.com/teimurjan)",
	"repository": "https://github.com/teimurjan/blazediff",
	"homepage": "https://blazediff.dev",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^24.3.0",
		"@vitest/ui": "^3.2.4",
		"ssim.js": "^3.5.0",
		"tsup": "8.5.0",
		"typescript": "5.9.2",
		"vitest": "^3.2.4"
	}
}
