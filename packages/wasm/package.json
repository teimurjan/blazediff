{
  "name": "@blazediff/wasm",
  "version": "0.7.2",
  "description": "Blazing-fast SIMD-optimized WASM implementation of BlazeDiff image comparison algorithm",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "pkg"
  ],
  "scripts": {
    "build": "pnpm build:wasm && tsup",
    "build:wasm": "wasm-pack build --target nodejs --release --out-dir pkg",
    "build:dev": "wasm-pack build --target nodejs --dev --out-dir pkg && tsup",
    "dev": "pnpm build:dev && tsup --watch",
    "clean": "rm -rf dist pkg pkg-nodejs target",
    "test": "wasm-pack test --headless --firefox"
  },
  "keywords": [
    "image",
    "comparison",
    "diff",
    "pixel",
    "visual-testing",
    "wasm",
    "simd",
    "rust"
  ],
  "author": "Teimur Gasanov <me@teimurjan.dev> (https://github.com/teimurjan)",
  "repository": "https://github.com/teimurjan/blazediff",
  "homepage": "https://github.com/teimurjan/blazediff",
  "license": "MIT",
  "dependencies": {
    "@blazediff/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  }
}
