{
	"name": "@blazediff/bin",
	"version": "3.2.0",
	"description": "Native Rust binaries for blazediff - the fastest image diff in the world",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"typecheck": "tsc --noEmit",
		"build": "tsup",
		"clean": "rm -rf dist",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"optionalDependencies": {
		"@blazediff/bin-darwin-arm64": "3.2.0",
		"@blazediff/bin-darwin-x64": "3.2.0",
		"@blazediff/bin-linux-arm64": "3.2.0",
		"@blazediff/bin-linux-x64": "3.2.0",
		"@blazediff/bin-win32-arm64": "3.2.0",
		"@blazediff/bin-win32-x64": "3.2.0"
	},
	"keywords": [
		"image",
		"comparison",
		"diff",
		"pixel",
		"visual-testing",
		"visual-regression",
		"screenshot",
		"native",
		"rust",
		"simd",
		"fast"
	],
	"author": "Teimur Gasanov <me@teimurjan.dev> (https://github.com/teimurjan)",
	"repository": "https://github.com/teimurjan/blazediff",
	"homepage": "https://blazediff.dev",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^24.3.0",
		"tsup": "8.5.0",
		"typescript": "5.9.2",
		"vitest": "^3.2.4"
	}
}
